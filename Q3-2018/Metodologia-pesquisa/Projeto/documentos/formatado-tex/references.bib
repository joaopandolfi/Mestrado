journal = {Infection, Genetics and Evolution},
keywords = {Culicidae,Landmarks,Multivariate analysis,Polymorphism,Variation,Wing},
mendeley-groups = {Mestrado/Revis{\~{a}}o sistem{\'{a}}tica},
pages = {205--215},
pmid = {28673547},
publisher = {Elsevier B.V},
title = {{Geometric morphometrics in mosquitoes: What has been measured?}},
url = {http://dx.doi.org/10.1016/j.meegid.2017.06.029},
volume = {54},
