doi = {10.1080/08839514.2012.731342},
file = {:C$\backslash$:/Users/joaop/Google Drive/UFABC/Mestrado/Q2/Metodologia-pesquisa/trabalho/base/A hierarchical artificial neural.pdf:pdf},
